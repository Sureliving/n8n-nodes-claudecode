{
  "name": "@sureliving/n8n-nodes-claudecode",
  "version": "0.1.3",
  "description": "n8n node for Claude Code SDK integration with MCP support",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "claude",
    "claude-code",
    "ai",
    "anthropic",
    "mcp",
    "model-context-protocol"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sureliving/n8n-nodes-claudecode#readme",
  "bugs": {
    "url": "https://github.com/sureliving/n8n-nodes-claudecode/issues"
  },
  "author": {
    "name": "sureliving"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sureliving/n8n-nodes-claudecode.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/nodes/ClaudeCode/ClaudeCodeCreds.node.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "npm run format:check && npx rimraf dist && tsc && gulp build:icons && gulp build:schemas",
    "format:check": "prettier nodes credentials --check",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "commit": "cz"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/ClaudeCode/ClaudeCodeCreds.node.js"
    ],
    "credentials": [
      "dist/credentials/AnthropicApi.credentials.js",
      "dist/credentials/GitlabApi.credentials.js"
    ]
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^22.13.10",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "@typescript-eslint/parser": "~8.32.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-workflow": "*",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
